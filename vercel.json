{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/api/cities", "dest": "/api/cities" },
    { "src": "/api/tours", "dest": "/api/tours" },
    { "src": "/api/tour-details", "dest": "/api/tour-details" },
    { "src": "/api/tour-options", "dest": "/api/tour-options" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
